{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python@3.1.31",
            "config": {
                "maxLambdaSize": "15mb",
                "includeFiles": ["templates/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
} 